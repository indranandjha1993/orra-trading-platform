# App
ENV=development
LOG_LEVEL=INFO

# Clerk
CLERK_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxx
CLERK_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxx
CLERK_JWKS_URL=https://example.clerk.accounts.dev/.well-known/jwks.json
CLERK_ISSUER=https://example.clerk.accounts.dev
CLERK_AUDIENCE=
CLERK_API_BASE_URL=https://api.clerk.com/v1

# Kite Connect
KITE_API_KEY=your_kite_api_key
KITE_API_SECRET=your_kite_api_secret
KITE_REDIRECT_URL=http://localhost:8000/auth/kite/callback

# Master encryption key (must be Fernet key, generate via: uv run python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
MASTER_ENCRYPTION_KEY=replace_with_fernet_key

# Primary PostgreSQL (tenant data)
DATABASE_URL=postgresql+asyncpg://orra_user:orra_password@localhost:5432/orra_app
POSTGRES_DB=orra_app
POSTGRES_USER=orra_user
POSTGRES_PASSWORD=orra_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis
REDIS_URL=redis://localhost:6379/0

# TimescaleDB (market data history)
TIMESCALE_DB=orra_market
TIMESCALE_USER=orra_market_user
TIMESCALE_PASSWORD=orra_market_password
TIMESCALE_HOST=localhost
TIMESCALE_PORT=5433

# Agents
AUTH_REFRESH_INTERVAL_SECONDS=86400
AUTH_TOKEN_TTL_SECONDS=72000
AUTH_MAX_RETRY_DELAY_SECONDS=300
PLAYWRIGHT_HEADLESS=true
ZERODHA_USER_SELECTOR=input#userid
ZERODHA_PASSWORD_SELECTOR=input#password
ZERODHA_TOTP_SELECTOR=input#totp
ZERODHA_SUBMIT_SELECTOR=button[type='submit']
# JSON maps keyed by tenant UUID
ZERODHA_USER_ID_MAP_JSON={}
ZERODHA_PASSWORD_MAP_JSON={}
TICKER_RECONNECT_INITIAL_DELAY_SECONDS=2
TICKER_RECONNECT_MAX_DELAY_SECONDS=120
TICKER_INSTRUMENT_TOKENS_CSV=
SUPER_ADMIN_SUBJECTS_CSV=

# Billing
STRIPE_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxxxxxxxxxx
CLERK_WEBHOOK_SECRET=
BASIC_STRATEGY_LIMIT=1
BASIC_DAILY_TRADE_LIMIT=5

# Notification Engine
NOTIFICATION_CONSUMER_GROUP=notifications
NOTIFICATION_CONSUMER_NAME=worker-1
EXECUTION_RESULTS_STREAM_NAME=execution_results
AUTH_ERROR_STREAM_NAME=auth_errors
NOTIFICATION_FAILURE_STREAM_NAME=notification_failures
NOTIFICATION_STREAM_BLOCK_MS=5000
N8N_TELEGRAM_WEBHOOK_URL=
N8N_WHATSAPP_WEBHOOK_URL=
N8N_EMAIL_WEBHOOK_URL=
N8N_URGENT_WEBHOOK_URL=
