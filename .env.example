# App
ENV=development
LOG_LEVEL=INFO

# Clerk
CLERK_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxx
CLERK_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxx
CLERK_JWKS_URL=https://example.clerk.accounts.dev/.well-known/jwks.json
CLERK_ISSUER=https://example.clerk.accounts.dev
CLERK_AUDIENCE=

# Kite Connect
KITE_API_KEY=your_kite_api_key
KITE_API_SECRET=your_kite_api_secret
KITE_REDIRECT_URL=http://localhost:8000/auth/kite/callback

# Master encryption key (must be Fernet key, generate via: uv run python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
MASTER_ENCRYPTION_KEY=replace_with_fernet_key

# Primary PostgreSQL (tenant data)
DATABASE_URL=postgresql+asyncpg://orra_user:orra_password@localhost:5432/orra_app
POSTGRES_DB=orra_app
POSTGRES_USER=orra_user
POSTGRES_PASSWORD=orra_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis
REDIS_URL=redis://localhost:6379/0

# TimescaleDB (market data history)
TIMESCALE_DB=orra_market
TIMESCALE_USER=orra_market_user
TIMESCALE_PASSWORD=orra_market_password
TIMESCALE_HOST=localhost
TIMESCALE_PORT=5433
